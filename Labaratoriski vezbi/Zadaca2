#include <iostream>

//Напиши програма во која се внесуваат 4ри броеви. Доколку збирот на првите 2 е парен и
//збирот на последните 2 броја е непарен да се испечати 1, во спротивно да се испечати 0.

using namespace std;

int main() {

    int a, b, c, d;
    cin>>a>>b>>c>>d;

    int Zbir1 = a + b;
    int Zbir2 = c + d;

    if((Zbir1 % 2 == 0) && (Zbir2 % 2 == 0)){
        cout<<"1"<<endl;
    }
    else {
        cout<<"0"<<endl;
    }
    return 0;
}
