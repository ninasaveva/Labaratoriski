#include <iostream>
using namespace std;

//Да се напише програма што ќе ја испечати сумата на сите броеви деливи со З и не
//делливи со 2 во интервалот [А,В). А В се внесуваат од тастатура
//Објаснување на примерот.
//А=10, В = 20. Броеви кои припаѓаат во интервалот се 10,11,12,13,.19. Од нив деллив со З
// а не деллив со 2 е бројот 15, па сумата е 15.


int main() {
    int A, B, suma=0;
    cin >> A >> B;
    int i;
    for( i=A; i<B; i++){

        if(i % 3 == 0 && i % 2 != 0 ){

            suma = suma + i;
        }

    }  cout<<suma<<endl;



    return 0;
}
